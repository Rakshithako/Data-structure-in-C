#include <stdio.h>
#include <stdlib.h>

int main() {
    int *p;
    p = (int*) malloc(sizeof(int));  // allocate memory for 1 integer
    *p = 5;                          // store value
    printf("%d\n", *p);              // print value
    free(p);                         // free memory
    return 0;
}

 
